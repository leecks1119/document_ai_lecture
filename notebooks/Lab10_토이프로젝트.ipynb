        "# Document Processor ?¥Îûò??(?µÌï©)\n",
        "class DocumentProcessor:\n",
        "    def __init__(self):\n",
        "        self.ocr = PaddleOCR(use_textline_orientation=True, lang='korean')\n",
        "        \n",
        "    def process(self, image_path):\n",
        "        \"\"\"?¥Î?ÏßÄ Ï≤òÎ¶¨ ?åÏù¥?ÑÎùº??"\"\"\n",
        "        results = {}\n",
        "        \n",
        "        # 1. OCR ?§Ìñâ\n",
        "        ocr_result = self.ocr.ocr(image_path, cls=True)\n",
        "        full_text = ' '.join([line[1][0] for line in ocr_result[0]])\n",
        "        results['full_text'] = full_text\n",
        "        \n",
        "        # 2. ?ïÎ≥¥ Ï∂îÏ∂ú (NER)\n",
        "        entities = self._extract_entities(full_text)\n",
        "        results['entities'] = entities\n",
        "        \n",
        "        # 3. ?†Î¢∞??Î∂ÑÏÑù\n",
        "        confidences = [line[1][1] for line in ocr_result[0]]\n",
        "        results['avg_confidence'] = sum(confidences) / len(confidences) if confidences else 0\n",
        "        \n",
        "        return results\n",
        "    \n",
        "    def _extract_entities(self, text):\n",
        "        \"\"\"Í∑úÏπô Í∏∞Î∞ò ?ïÎ≥¥ Ï∂îÏ∂ú\"\"\"\n",
        "        entities = {}\n",
        "        \n",
        "        # ?†Ïßú\n",
        "        date_pattern = r'(\\\\d{4})??\\\s*(\\\\d{1,2})??\\\s*(\\\\d{1,2})??\n",
        "        date_match = re.search(date_pattern, text)\n",
        "        if date_match:\n",
        "            entities['?†Ïßú'] = date_match.group(0)\n",
        "        \n",
        "        # Í∏àÏï°\n",
        "        amount_pattern = r'([\\\\d,]+)??\n",
        "        amount_match = re.search(amount_pattern, text)\n",
        "        if amount_match:\n",
        "            entities['Í∏àÏï°'] = amount_match.group(0)\n",
        "        \n",
        "        # ?ÑÌôîÎ≤àÌò∏\n",
        "        phone_pattern = r'(\\\\d{2,3}-\\\\d{3,4}-\\\\d{4})'\n",
        "        phone_match = re.search(phone_pattern, text)\n",
        "        if phone_match:\n",
        "            entities['?ÑÌôîÎ≤àÌò∏'] = phone_match.group(0)\n",
        "        \n",
        "        # ?¥Î©î??n",
        "        email_pattern = r'([\\\\w\\\\.-]+@[\\\\w\\\\.-]+\\\\.[\\\\w]+)'\n",
        "        email_match = re.search(email_pattern, text)\n",
        "        if email_match:\n",
        "            entities['?¥Î©î??] = email_match.group(0)\n",
        "        \n",
        "        return entities\n",
        "\n",
        "processor = DocumentProcessor()\n",
        "print(\"??Document Processor Ï§ÄÎπ??ÑÎ£å!\")\n"
