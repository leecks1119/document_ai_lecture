        "# Tesseractë¡?? ë¢°??ì¸¡ì •\n",
        "import pytesseract\n",
        "from pytesseract import Output\n",
        "import pandas as pd\n",
        "\n",
        "def analyze_confidence(image_path, label):\n",
        "    img = cv2.imread(image_path)\n",
        "    data = pytesseract.image_to_data(img, lang='kor+eng', output_type=Output.DICT)\n",
        "    \n",
        "    # ? ë¢°??ë¶„ì„\n",
        "    confidences = [int(conf) for conf in data['conf'] if int(conf) > 0]\n",
        "    texts = [text for text in data['text'] if text.strip()]\n",
        "    \n",
        "    print(f\"\\n{'='*60}\")\n",
        "    print(f\"?“Š {label}\")\n",
        "    print(f\"{'='*60}\")\n",
        "    print(f\"  ?‰ê·  ? ë¢°?? {np.mean(confidences):.1f}%\")\n",
        "    print(f\"  ìµœì†Œ ? ë¢°?? {np.min(confidences):.1f}%\")\n",
        "    print(f\"  ìµœë? ? ë¢°?? {np.max(confidences):.1f}%\")\n",
        "    print(f\"  ?€? ë¢°??<70%) ?¨ì–´ ?? {sum(1 for c in confidences if c < 70)}\")\n",
        "    print(f\"\\n  ?¸ì‹???ìŠ¤?? {' '.join(texts)}\")\n",
        "    \n",
        "    return confidences\n",
        "\n",
        "conf_clean = analyze_confidence('clean.jpg', 'ê¹¨ë—???´ë?ì§€')\n",
        "conf_noisy = analyze_confidence('noisy.jpg', '?¸ì´ì¦??´ë?ì§€')\n"
      ]
