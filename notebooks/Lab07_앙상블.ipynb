        "# 3ê°?OCR ?”ì§„?¼ë¡œ ?¸ì‹\n",
        "import pytesseract\n",
        "from paddleocr import PaddleOCR\n",
        "import easyocr\n",
        "\n",
        "img = cv2.imread('test_doc.jpg')\n",
        "\n",
        "# 1. Tesseract\n",
        "text_tess = pytesseract.image_to_string(img, lang='kor+eng')\n",
        "\n",
        "# 2. PaddleOCR (ìµœì‹  API)\n",
        "paddle_ocr = PaddleOCR(use_textline_orientation=True, lang='korean')\n",
        "result_paddle = paddle_ocr.ocr('test_doc.jpg', cls=True)\n",
        "text_paddle = ' '.join([line[1][0] for line in result_paddle[0]])\n",
        "\n",
        "# 3. EasyOCR\n",
        "easy_reader = easyocr.Reader(['ko', 'en'], gpu=False)\n",
        "result_easy = easy_reader.readtext('test_doc.jpg')\n",
        "text_easy = ' '.join([item[1] for item in result_easy])\n",
        "\n",
        "print(\"=\"*60)\n",
        "print(\"?“Š ê°œë³„ OCR ?”ì§„ ê²°ê³¼\")\n",
        "print(\"=\"*60)\n",
        "print(f\"\\n1ï¸âƒ£ Tesseract:\\n{text_tess}\")\n",
        "print(f\"\\n2ï¸âƒ£ PaddleOCR:\\n{text_paddle}\")\n",
        "print(f\"\\n3ï¸âƒ£ EasyOCR:\\n{text_easy}\")\n",
        "print(\"=\"*60)\n"
      ]
    },
    {
