      "outputs": [],
      "source": [
        "# ?œê? ?¬í•¨???˜í”Œ ?´ë?ì§€ ?ì„±\n",
        "from PIL import Image, ImageDraw, ImageFont\n",
        "\n",
        "img = Image.new('RGB', (800, 400), color='white')\n",
        "draw = ImageDraw.Draw(img)\n",
        "\n",
        "# ?œëª©\n",
        "draw.text((30, 20), \"ê²¬ì ??", fill='black')\n",
        "\n",
        "# ??ê·¸ë¦¬ê¸?n",
        "table_data = [\n",
        "    [\"?ˆëª©\", \"?˜ëŸ‰\", \"?¨ê?\", \"ê¸ˆì•¡\"],\n",
        "    [\"?¸íŠ¸ë¶?", \"10\", \"1,500,000\", \"15,000,000\"],\n",
        "    [\"ëª¨ë‹ˆ??", \"20\", \"300,000\", \"6,000,000\"],\n",
        "    [\"?¤ë³´??", \"30\", \"50,000\", \"1,500,000\"],\n",
        "]\n",
        "\n",
        "x_start, y_start = 50, 70\n",
        "cell_width = 180\n",
        "cell_height = 40\n",
        "\n",
        "# ?€ ê·¸ë¦¬ê¸?n",
        "for i, row in enumerate(table_data):\n",
        "    for j, cell in enumerate(row):\n",
        "        x = x_start + j * cell_width\n",
        "        y = y_start + i * cell_height\n",
        "        # ?Œë‘ë¦?n",
        "        draw.rectangle([x, y, x+cell_width, y+cell_height], outline='black', width=2)\n",
        "        # ?ìŠ¤??n",
        "        draw.text((x+10, y+10), cell, fill='black')\n",
        "\n",
        "img.save('table_doc.jpg')\n",
        "print(\"?????´ë?ì§€ ?ì„± ?„ë£Œ!\")\n",
        "img\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "# OpenCVë¡????ì—­ ê²€ì¶?n",
        "img = cv2.imread('table_doc.jpg')\n",
        "gray = cv2.cvtColor(img, cv2.COLOR_BGR2GRAY)\n",
        "\n",
        "# ?´ì§„??n",
        "_, binary = cv2.threshold(gray, 150, 255, cv2.THRESH_BINARY_INV)\n",
        "\n",
        "# ?˜í‰??ê²€ì¶?n",
        "horizontal_kernel = cv2.getStructuringElement(cv2.MORPH_RECT, (40, 1))\n",
        "horizontal_lines = cv2.morphologyEx(binary, cv2.MORPH_OPEN, horizontal_kernel, iterations=2)\n",
        "\n",
        "# ?˜ì§??ê²€ì¶?n",
        "vertical_kernel = cv2.getStructuringElement(cv2.MORPH_RECT, (1, 40))\n",
        "vertical_lines = cv2.morphologyEx(binary, cv2.MORPH_OPEN, vertical_kernel, iterations=2)\n",
        "\n",
        "# ??êµ¬ì¡° ê²°í•©\n",
        "table_structure = cv2.add(horizontal_lines, vertical_lines)\n",
        "\n",
        "# ?œê°??n",
        "fig, axes = plt.subplots(1, 3, figsize=(15, 5))\n",
        "axes[0].imshow(cv2.cvtColor(img, cv2.COLOR_BGR2RGB))\n",
        "axes[0].set_title('?ë³¸ ?´ë?ì§€')\n",
        "axes[0].axis('off')\n",
        "\n",
        "axes[1].imshow(horizontal_lines, cmap='gray')\n",
        "axes[1].set_title('?˜í‰??ê²€ì¶?)\n",
        "axes[1].axis('off')\n",
        "\n",
        "axes[2].imshow(vertical_lines, cmap='gray')\n",
        "axes[2].set_title('?˜ì§??ê²€ì¶?)\n",
        "axes[2].axis('off')\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "print(\"????êµ¬ì¡° ê²€ì¶??„ë£Œ!\")\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "# PaddleOCRë¡??ìŠ¤??ì¶”ì¶œ\n",
        "ocr = PaddleOCR(use_textline_orientation=True, lang='korean')\n",
        "result = ocr.ocr('table_doc.jpg', cls=True)\n",
        "\n",
        "# ì¢Œí‘œ ê¸°ì??¼ë¡œ ?•ë ¬ (?„â†’?„ë˜, ?¼ìª½?’ì˜¤ë¥¸ìª½)\n",
        "ocr_results = []\n",
        "for line in result[0]:\n",
        "    bbox = line[0]\n",
        "    text = line[1][0]\n",
        "    conf = line[1][1]\n",
        "    \n",
        "    # ì¤‘ì‹¬??ê³„ì‚°\n",
        "    x_center = (bbox[0][0] + bbox[2][0]) / 2\n",
        "    y_center = (bbox[0][1] + bbox[2][1]) / 2\n",
        "    \n",
        "    ocr_results.append({\n",
        "        'text': text,\n",
        "        'x': x_center,\n",
        "        'y': y_center,\n",
        "        'confidence': conf\n",
        "    })\n",
        "\n",
        "# Y ì¢Œí‘œë¡?ë¨¼ì? ?•ë ¬ (??, ê·??¤ìŒ X ì¢Œí‘œë¡??•ë ¬ (??\n",
        "ocr_results_sorted = sorted(ocr_results, key=lambda r: (r['y'], r['x']))\n",
        "\n",
        "print(\"\\n?“‹ ì¶”ì¶œ???ìŠ¤??(?œì„œ?€ë¡?:\")\n",
        "for i, item in enumerate(ocr_results_sorted, 1):\n",
        "    print(f\"{i:2d}. {item['text']:20s} (? ë¢°?? {item['confidence']:.2%})\")\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "# DataFrame?¼ë¡œ ë³€??(4??êµ¬ì¡°)\n",
        "texts = [item['text'] for item in ocr_results_sorted]\n",
        "\n",
        "# 4ê°œì”© ë¬¶ì–´??DataFrame ?ì„±\n",
        "rows = []\n",
        "for i in range(0, len(texts), 4):\n",
        "    if i + 4 <= len(texts):\n",
        "        rows.append(texts[i:i+4])\n",
        "\n",
        "df = pd.DataFrame(rows[1:], columns=rows[0])\n",
        "\n",
        "print(\"\\n?“Š ???°ì´??(DataFrame):\")\n",
        "print(\"=\"*70)\n",
        "print(df.to_string(index=False))\n",
        "print(\"=\"*70)\n",
        "\n",
        "# CSVë¡??€??n",
        "df.to_csv('extracted_table.csv', index=False, encoding='utf-8-sig')\n",
        "print(\"\\n??CSV ?€???„ë£Œ: extracted_table.csv\")\n"
      ]
